<Window x:Class="PRAK5.Manager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRAK5"
        mc:Ignorable="d"
        Title="Manager" d:DesignWidth="932.901" Height="451">
    <Grid>
        <Button Content="Заказчики" Background="AliceBlue" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="31" Width="86" Click="CustomersClick" />
        <Frame x:Name="Page" Margin="110,0,0,0"/>
