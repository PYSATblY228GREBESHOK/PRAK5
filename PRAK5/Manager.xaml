        <Button Content="История операций" Background="AliceBlue" HorizontalAlignment="Left" Margin="10,294,0,0" VerticalAlignment="Top" Height="33" Width="86" Click="HistoryClick" FontSize="9"/>
        <Button Content="Поставщики" Background="AliceBlue" HorizontalAlignment="Left" Margin="10,332,0,0" VerticalAlignment="Top" Height="26" Width="86" Click="Button_Click_1"/>
        <Button Content="Инвентарь" Background="AliceBlue" HorizontalAlignment="Left" Margin="10,363,0,0" VerticalAlignment="Top" Height="21" Width="86"/>
    </Grid>
</Window>
