        <Button Content="Выход" Background="Red" HorizontalAlignment="Left" Margin="10,399,0,0" VerticalAlignment="Top" Click="СloseManagerClick" Width="86" Height="26"/>
        <Button Content="Аккаунты" Background="AliceBlue" HorizontalAlignment="Left" Margin="10,82,0,0" VerticalAlignment="Top" Height="28" Width="86" Click="Button_Click"/>
        <Button Content="Отзывы" Background="AliceBlue" HorizontalAlignment="Left" Margin="10,115,0,0" VerticalAlignment="Top" Height="27" Width="86" Click="ReviewsClick" />
